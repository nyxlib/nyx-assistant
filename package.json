{
    "name": "indi-assistant",
    "description": "INDI Assistant",
    "version": "1.0.0",
    "type": "module",
    "private": true,
    "scripts": {
        "app-dev": "cd ./app/ && ../node_modules/vite/bin/vite.js dev",
        "app-prod": "cd ./app/ && ../node_modules/vite/bin/vite.js build",
        "app-preview": "cd ./app/ && ../node_modules/vite/bin/vite.js preview",
        "tauri-dev": "cd ./tauri/ && ../node_modules/@tauri-apps/cli/tauri.js dev",
        "tauri-prod": "cd ./tauri/ && ../node_modules/@tauri-apps/cli/tauri.js build",
        "lint": "cd ./app/ && ../node_modules/eslint/bin/eslint.js"
    },
    "dependencies": {
        "@tauri-apps/api": "^2.0.0-beta.15",
        "@tauri-apps/plugin-dialog": "^2.0.0-beta.7",
        "@tauri-apps/plugin-fs": "^2.0.0-beta.7",
        "@tauri-apps/plugin-notification": "^2.0.0-beta.7",
        "bootstrap": "^5.3.3",
        "bootstrap-icons": "^1.11.3",
        "uuid": "^9.0.1",
        "vue": "^3.4.34"
    },
    "devDependencies": {
        "@tauri-apps/cli": "^2.0.0-beta.22",
        "@vitejs/plugin-vue": "^5.1.1",
        "@vue/eslint-config-prettier": "^9.0.0",
        "eslint": "^9.8.0",
        "eslint-plugin-vue": "^9.27.0",
        "sass": "^1.77.8",
        "vite": "^5.3.5",
        "vite-plugin-eslint": "^1.8.1"
