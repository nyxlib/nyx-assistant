{
    "name": "nyx-assistant",
    "description": "Nyx Assistant",
    "version": "1.0.0",
    "type": "module",
    "private": true,
    "scripts": {
        "app-dev": "./gen/build.sh && cd ./app/ && ../node_modules/vite/bin/vite.js --port 2468 dev",
        "app-prod": "./gen/build.sh && cd ./app/ && ../node_modules/vite/bin/vite.js build",
        "tauri-dev": "cd ./tauri/ && ../node_modules/@tauri-apps/cli/tauri.js dev",
        "tauri-prod": "cd ./tauri/ && ../node_modules/@tauri-apps/cli/tauri.js build",
        "lint": "cd ./app/ && ../node_modules/eslint/bin/eslint.js"
    },
    "dependencies": {
        "@tauri-apps/api": "2.0.0-rc.1",
        "@tauri-apps/plugin-dialog": "^2.0.0-rc.0",
        "@tauri-apps/plugin-fs": "^2.0.0-rc.1",
        "@tauri-apps/plugin-notification": "^2.0.0-rc.0",
        "@tauri-apps/plugin-shell": "^2.0.0-rc.0",
        "bootstrap": "^5.3.3",
        "bootstrap-icons": "^1.11.3",
        "uuid": "^10.0.0",
        "vue": "^3.4.38"
    },
    "devDependencies": {
        "@tauri-apps/cli": "2.0.0-rc.4",
        "@vitejs/plugin-vue": "^5.1.2",
        "@vue/eslint-config-prettier": "^9.0.0",
        "eslint": "^9.9.0",
        "eslint-plugin-vue": "^9.27.0",
        "sass": "^1.77.8",
        "vite": "^5.4.1",
        "vite-plugin-eslint": "^1.8.1"
    },
    "author": "Jérôme Odier",
    "license": "CeCILL",
    "repository": {
        "type": "git",
        "url": "https://gitlab.in2p3.fr/lpsc-kid/indi-dashboard"
    }
}
